{
  "name": "gms-notebook-file-server",
  "version": "1.0.0",
  "description": "Local file server for GMsNotebook.com",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "concurrently \"tsc --watch\" \"nodemon -q dist/index.js\"",
    "test-server": "cross-env NODE_ENV=test PORT=3001 LOCAL_FOLDER=./test-files npm run start",
    "test-runner": "cross-env PORT=3001 jest",
    "test": "npm run build && concurrently -s command-1 -k npm:test-server npm:test-runner"
  },
  "author": "David Prothero",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "junk": "^4.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.1",
    "@types/node": "^18.15.11",
    "concurrently": "^8.0.1",
    "cross-env": "^7.0.3",
    "jest": "^29.5.0",
    "nodemon": "^2.0.22",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  }
}
